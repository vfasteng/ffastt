@model FastEngSite.Controllers.MainPageData
@{
    ViewBag.Title = "FASTEngineeringAnalysis";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script type="text/javascript" src="Scripts/fuse.js"></script>
	<script type="text/javascript" src="Scripts/jquery-3.1.1.min.js"></script>
	<script type="text/javascript" src="Scripts/search.js"></script>
	<script type="text/javascript" src="Scripts/searchRouting.js"></script>

<div id="wrapper" class="container" ng-controller="adminCtrl">

    <div class="mainheader">
        @Html.Partial("~/Views/ViewAccountMenu.cshtml")

        <div class="searchblock">
            <input type="text" id="searchTerm" class="searchTerm" placeholder="Search...">
            <button onclick="searchFastVids()" class="searchicon" style="border: 0; background: transparent; display: inline-block">
                <img src="images/searchicon.png" width="20" height="20" alt="submit" />
            </button>
        </div>

        <div class="navigation">
            @Html.Partial("~/Views/MenuNavigation.cshtml")
        </div>

    </div>

    <div class="firstblock row bottom0">
        <div class="col-md-6 top30">
            <div>
                <img src="images/logo.png" width="201" height="79" style="padding-bottom: 10px" alt="" />
            </div>
            <div>
                <iframe width="430" height="275" src="https://www.youtube.com/embed/fomfXPi3J0A?rel=0" frameborder="0" allowfullscreen></iframe>
            </div>
        </div>

        <div class="col-md-6 top30">
            <div class="admin-image-block">
                <p>
                    Manage your files, Access Free Downloads, And Much More.<br>
                    <strong>fastengineeringanalysis.com</strong>
                </p>
            </div>
        </div>
    </div>
    @if (Request.IsAuthenticated)
    {
        <div class="secondblock row bottom10">
            <div class="col-md-12">
                <button class="btn btn-default btn-lg left30 bottom30" id="adminFileUploadBtn">Upload logo</button>
                <div>
                    <div id="adminFileProgressOuter" class="progress progress-striped active" style="display: none;">
                        <div id="adminFileProgressBar" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <img src="Admin/GetLogo/" ng-show="vm.ShowLogo" id="admin-logo" />
            </div>
        </div>
    }

    <div class="secondblock row">
        <div class="quarterblock col-md-3">
            <div class="whole">
                <div class="type-mid">
                    <p>Storage Explorer</p>
                </div>
                <div class="plan">
                    <div class="content">
                        <ul>
                            <li>Manage Storage Files</li>
                            <li>
                                @if (!Request.IsAuthenticated)
                                {
                                    <a href="#" class="btn-need-auth">
                                        <img src="images/go.png" width="129" height="37" style="padding-top: 9px" alt="" />
                                    </a>
                                }
                                else
                                {
                                    <a href="/Storage">
                                        <img src="images/go.png" width="129" height="37" style="padding-top: 9px" alt="" />
                                    </a>
                                }
                            </li>
                        </ul>

                    </div>
                </div>
            </div>
        </div>
        <div class="quarterblock col-md-3">
            <div class="whole">
                <div class="type-mid">
                    <p>Create new account</p>
                </div>
                <div class="plan">
                    <div class="content">
                        <ul>
                            <li>Multipe users</li>
                            <li>
                                <a href="/Login/CreateAccount">
                                    <img src="images/go.png" width="129" height="37" style="padding-top: 9px" alt="" />
                                </a>
                            </li>
                        </ul>

                    </div>
                </div>
            </div>
        </div>

        <div class="quarterblock col-md-3">
            <div class="whole">
                <div class="type-mid">
                    <p>Increased Storage</p>
                </div>
                <div class="plan">
                    <div class="content">
                        <ul>
                            <li>50GB for Â£50 per year</li>
                            <li>
                                @if (!Request.IsAuthenticated)
                                {
                                    <a href="#" class="btn-need-auth">
                                        <img src="images/buynow.png" width="129" height="37" style="padding-top: 9px" alt="" />
                                    </a>
                                }
                                else
                                {
                                    <form action="@Model.PaypalUrl" method="post" class="top10">
                                        <input type="hidden" name="cmd" value="_xclick" />
                                        <input type="hidden" name="business" value="@Model.PayPalBusiness" />
                                        <input type="hidden" name="currency_code" value="EUR">
                                        <input type="hidden" name="item_name" value="ProductStorage50GB" />
                                        <input type="hidden" name="amount" value="50.00" />
                                        <input type="image" src="/Images/buynow.png" alt="Submit Form" />
                                    </form>
                                }
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="quarterblock col-md-3">
            <div class="whole">
                <div class="type-mid">
                    <p>Visual Studio</p>
                </div>
                <div class="plan">
                    <div class="content">
                        <ul>
                            <li>Free Download Link</li>
                            <li>
                                <a href="https://www.visualstudio.com/en-us/products/free-developer-offers-vs.aspx" target="_blank">
                                    <img src="images/go.png" width="129" height="37" style="padding-top: 9px" alt="" />
                                </a>
                            </li>
                        </ul>

                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="secondblock row top20">

        <div class="quarterblock col-md-3">
            <div class="whole">
                <div class="type-mid">
                    <p>3D Scanning Software</p>
                </div>
                <div class="plan">
                    <div class="content">
                        <ul>
                            <li>Free Download Link</li>
                            <li>
                                <a href="http://www.kscan3d.com" target="_blank">
                                    <img src="images/go.png" width="129" height="37" style="padding-top: 9px" alt="" />
                                </a>
                            </li>
                        </ul>

                    </div>
                </div>
            </div>
        </div>
        <div class="quarterblock col-md-3">
            <div class="whole">
                <div class="type-mid">
                    <p>Material Tables</p>
                </div>
                <div class="plan">
                    <div class="content">
                        <ul>
                            <li>Quick-View Tables</li>
                            <li>
                                <a href="/Materials">
                                    <img src="images/go.png" width="129" height="37" style="padding-top: 9px" alt="" />
                                </a>
                            </li>
                        </ul>

                    </div>
                </div>
            </div>
        </div>
        <div class="quarterblock col-md-3">
            <div class="whole">
                <div class="type-mid">
                    <p>FORUM</p>
                </div>
                <div class="plan">
                    <div class="content">
                        <ul>
                            <li>FAST Forum</li>
                            <li>
                                <a href="/Forum">
                                    <img src="images/go.png" width="129" height="37" style="padding-top: 9px" alt="" />
                                </a>
                            </li>
                        </ul>

                    </div>
                </div>
            </div>
        </div>
        <div class="quarterblock col-md-3">
            <div class="whole">
                <div class="type-mid">
                    <p>Youtube</p>
                </div>
                <div class="plan">
                    <div class="content">
                        <ul>
                            <li>fastengineeringanalysis</li>
                            <li>
                                <a href="http://www.youtube.com/channel/UCgCxkb0Jmnt-PUdDRFZbOYA" target="_blank">
                                    <img src="images/go.png" width="129" height="37" style="padding-top: 9px" alt="" />
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="grenadeslider">

    <!--Sheet Slider-->
    <div class="sheet-slider" id="sh-cinco">
        <input id="s1" class="slide" type="radio" name="slide" checked="checked" />
        <input id="s2" class="slide" type="radio" name="slide" />
        <input id="s3" class="slide" type="radio" name="slide" />
        <input id="s4" class="slide" type="radio" name="slide" />
        <input id="s5" class="slide" type="radio" name="slide" />
        <input id="s6" class="slide" type="radio" name="slide" />
        <div class="conten">
            <div class="tab">
                <img src="~/Images/Admin/3D_scanning1.jpg" />
                <div class="text">
                    <h3><a href="http://www.kscan3d.com">DOWNLOAD FREE SOFTWARE</a></h3>
                    <span>For use with a scanning device.</span>
                </div>
            </div>

            <div class="tab">
                <img src="~/Images/Admin/Forum.jpg" />
                <div class="text">
                    <h3><a href="/Forum">OPEN DISCUSSIONS</a></h3>
                    <span>Swap tips with other users.</span>
                </div>
            </div>

            <div class="tab">
                <img src="~/Images/Admin/Increased_Storage.jpg" />
                <div class="text">
                    <h3>
                        @if (!Request.IsAuthenticated)
                        {
                            <a href="#" class="btn-need-auth">RAISE YOUR CAPACITY</a>
                        }
                        else
                        {
                            <form id="paypal_storage_form_slider" action="@Model.PaypalUrl" method="post" class="top10">
                                <input type="hidden" name="cmd" value="_xclick" />
                                <input type="hidden" name="business" value="@Model.PayPalBusiness" />
                                <input type="hidden" name="currency_code" value="EUR">
                                <input type="hidden" name="item_name" value="ProductStorage50GB" />
                                <input type="hidden" name="amount" value="50.00" />
                                <a href="javascript:{}" onclick="document.getElementById('paypal_storage_form_slider').submit(); return false;">RAISE YOUR CAPACITY</a>
                            </form>
                        }
                    </h3>
                    <span>Swap tips with other users.</span>
                </div>
            </div>

            <div class="tab">
                <img src="~/Images/Admin/Material.jpg" />
                <div class="text">
                    <h3><a href="/Materials">QUICK-VIEW TABLES</a></h3>
                    <span>View properties offline.</span>
                </div>
            </div>

            <div class="tab">
                <img src="~/Images/Admin/StorageExplorer.jpg" />
                <div class="text">
                    <h3><a href="/Storage">MANAGE YOUR FILES</a></h3>
                    <span>Without loading the Models.</span>
                </div>
            </div>

            <div class="tab">
                <img src="~/Images/Admin/Youtube.jpg" />
                <div class="text">
                    <h3><a href="http://www.youtube.com/channel/UCgCxkb0Jmnt-PUdDRFZbOYA">VIEW VIDEO TUTORIALS</a></h3>
                    <span>Get expert help on our own channel.</span>
                </div>
            </div>
        </div>

        <div class="sh-btns">
            <label for="s1"></label>
            <label for="s2"></label>
            <label for="s3"></label>
            <label for="s4"></label>
            <label for="s5"></label>
        </div>
    </div>
</div>
<div class="carotitle">
    <img src="images/ourwork.png" width="254" height="37" alt="" />
</div>

@if (Request.IsAuthenticated)
{
    <button id="id-open-gallery-setup" class="btn btn-default btn-lg left30 bottom30">
        <span class="cursor-pointer glyphicon glyphicon-cog" aria-hidden="true"></span>
        Setup Gallery
    </button>
}

@Html.Partial("~/Views/OurWorkSlider.cshtml")


<div class="mainfooter">

    <div class="navigation2">
        @Html.Partial("~/Views/MenuNavigation.cshtml")
    </div>
    <div class="bottomfooter">
        <div class="footerleft">
            <p>Terms & Conditions</p>
            <br>
            <p>
                Terms and conditions apply to all purchases
                <br>
                for full details please <a href="#">Read Now</a>
            </p>
            <br>
            <br>
            @Html.Partial("~/Views/Copyright.cshtml")
        </div>
        <div class="footerright">
            @Html.Partial("~/Views/SocialLinks.cshtml")
        </div>
    </div>
</div>




@Html.Partial("~/Views/CommonPage.cshtml")

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/plugins")
<script src="/Scripts/PureJs/jquery.horizontal.scroll.js" type="text/javascript"></script>

@Scripts.Render("~/js")
