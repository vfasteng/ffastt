@{
    ViewBag.Title = "FASTEngineeringAnalysis";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript" src="Scripts/fuse.js"></script>
	<script type="text/javascript" src="Scripts/jquery-3.1.1.min.js"></script>
	<script type="text/javascript" src="Scripts/search.js"></script>
	<script type="text/javascript" src="Scripts/searchRouting.js"></script>

<div id="wrapper" class="container">

    <div class="mainheader">
        @Html.Partial("~/Views/ViewAccountMenu.cshtml")

        <div class="searchblock">
            <input type="text" id="searchTerm" class="searchTerm" placeholder="Search...">
            <button onclick="searchFastVids()" class="searchicon" style="border: 0; background: transparent; display: inline-block">
                <img src="images/searchicon.png" width="20" height="20" alt="submit" />
            </button>
        </div>

        <div class="navigation">
            @Html.Partial("~/Views/MenuNavigation.cshtml")
        </div>

    </div>

    <div class="container">
        <h4 class="modal-title top80">Create New Account</h4>
        <div class="modal-body">
            <input id="register-username" class="form-control" type="text" placeholder="Email" required>
            <input id="register-password" class="form-control top20" type="password" placeholder="Password" required>
        </div>
        <div class="modal-footer">
            <button id="btn-register-ok" type="button" class="btn btn-default" data-dismiss="modal">Register</button>
        </div>
    </div>

</div>


@Html.Partial("~/Views/Home/ModalDialog.cshtml")
@Html.Partial("~/Views/Home/Login.cshtml")
@Html.Partial("~/Views/SimulationGallerySetup.cshtml")

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/plugins")

<script src="/Scripts/PureJs/jquery.horizontal.scroll.js" type="text/javascript"></script>
<script src="/Scripts/Typescript/UI/MessageBox.js" type="text/javascript"></script>
<script src="/Scripts/Typescript/ErrorHandler.js" type="text/javascript"></script>
<script src="/Scripts/Typescript/SignUpPage.js" type="text/javascript"></script>
